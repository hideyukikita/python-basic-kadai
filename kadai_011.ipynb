      "source": [
        "array = [\"水\",\"金\",\"地\",\"火\",\"木\",\"土\",\"天\",\"海\",\"冥\"]\n",
        "\n",
        "# forでのループ\n",
        "for i in array:\n",
        "  print(i)\n",
        "\n",
        "# whileでのループ\n",
        "# 要素数\n",
        "length = len(array)\n",
        "\n",
        "# 繰り返し回数\n",
        "cnt = 0\n",
        "\n",
        "while cnt <= length - 1:\n",
        "  print(array[cnt])\n",
        "  cnt += 1\n"
      ]
