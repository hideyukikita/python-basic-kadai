        "class Human:\n",
        "  # 属性を定義する\n",
        "  def __init__(self, name, age):\n",
        "    self.name = name\n",
        "    self.age = age\n",
        "  # 関数を定義する\n",
        "  def printinfo(self):\n",
        "    print(f\"name: {self.name}, age: {self.age}\")\n",
        "\n",
        "user = Human(\"侍太郎\", 26)\n",
        "user.printinfo()"
